from random import randint
def binary_search(data, elem):
    low = 0
    high = len(data) - 1
    while low <= high:
        middle = (low + high) // 2
        if data[middle] == elem:
            return middle
        elif data[middle] > elem:
            high = middle - 1
        else:
            low = middle + 1
    return -1
list_1 = [randint(1, 100) for i in range(10)]
print(' '.join(map(str, list_1)))
try:
    number = int(input('Введите число чтобы проверить есть ли оно в списке: '))
except ValueError:
    print('Введите числовое значение!')
    quit()
if binary_search(list_1, number) == -1:
    print('Такого числа нет в списке!!!')
else:
    print(f'Данное число есть в списке под индексом {binary_search(list_1, number)}')
